@model IEnumerable<ElectroStoreMVC.Models.User>
@{
    ViewData["Title"] = "LLL";
}
<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="generator" content="Hugo 0.80.0">
    <title>Carousel Template · Bootstrap v5.0</title>

    <link rel="canonical" href="https://getbootstrap.com/docs/5.0/examples/carousel/">



    <!-- Bootstrap core CSS -->
    <link href="/docs/5.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl" crossorigin="anonymous">

    <!-- Favicons -->
    <link rel="apple-touch-icon" href="/docs/5.0/assets/img/favicons/apple-touch-icon.png" sizes="180x180">
    <link rel="icon" href="/docs/5.0/assets/img/favicons/favicon-32x32.png" sizes="32x32" type="image/png">
    <link rel="icon" href="/docs/5.0/assets/img/favicons/favicon-16x16.png" sizes="16x16" type="image/png">
    <link rel="manifest" href="/docs/5.0/assets/img/favicons/manifest.json">
    <link rel="mask-icon" href="/docs/5.0/assets/img/favicons/safari-pinned-tab.svg" color="#7952b3">
    <link rel="icon" href="/docs/5.0/assets/img/favicons/favicon.ico">
    <meta name="theme-color" content="#7952b3">


    <!-- Custom styles for this template -->
    <link href="carousel.css" rel="stylesheet">
</head>
<body>


    <main>

        <!-- Marketing messaging and featurettes
        ================================================== -->
        <!-- Wrap the rest of the page in another container to center all the content. -->

        <div class="container marketing">

            <!-- Three columns of text below the carousel -->
            <div style="padding-top:5%;" class="row">
                <div class="col-lg-4">
                    <img src="~/images/iphone11-black-select-2019.png" style="width:50%;display:block;margin:0 auto;" alt="Айфон 11">

                    <h2 style="text-align:center;">Электроника</h2>
                    <p style="text-align:justify;">
                        Современные смартфоны, качественная ТВ и бытовая видеотехника является отличным 
                        способом для отдыха и увлекательного времяпрепровождения для всей семьи.
                    </p>
                    <p><a class="btn btn-secondary" href="#">Перейти &raquo;</a></p>
                </div><!-- /.col-lg-4 -->
                <div class="col-lg-4">
                    <img src="~/images/good_img_87200c74-64f6-11ea-80c7-005056840c70_600.jpg" style="width:60%;display:block;margin:0 auto;" alt="Айфон 11">

                    <h2 style="text-align:center;">Бытовая техника</h2>
                    <p style="text-align:justify;">
                        В нашем интернет-магазине представлен большой ассортимент
                        качественной мелкой и крупной бытовой техники на любой вкус.
                    </p>
                    <p><a class="btn btn-secondary" href="#">Перейти &raquo;</a></p>
                </div><!-- /.col-lg-4 -->
                <div class="col-lg-4">
                    <img src="~/images/mydc2ru-a-a2338-macbook-air-13-m1-2020-silver-noutbuk-apple-1_600.jpg" style="width:70%;display:block;margin:0 auto;" alt="Айфон 11">

                    <h2 style="text-align:center;">Компьютеры</h2>
                    <p style="text-align:justify;">
                        Наш интернет-магазин предлагает широчайший ассортимент компьютерной техники в
                        Минске с возможностью доставки по Беларуси.
                    </p>
                    <p><a class="btn btn-secondary" href="#">Перейти &raquo;</a></p>
                </div><!-- /.col-lg-4 -->
            </div><!-- /.row -->
            <!-- START THE FEATURETTES -->

            <hr class="featurette-divider">

            <div class="row featurette">
                <div class="col-md-7">
                    <h2 class="featurette-heading">Стиральные машины нового поколоения. <span class="text-muted">Технология AI DD.</span></h2>
                    <p class="lead">
                        Усовершенствованный двигатель с искусственным интеллектом позволяет с точностью настроить режимы стирки,
                        автоматически определяя вес белья и с точностью оценивая мягкость ткани.
                        В память стиральной машины заложена база данных из 20 000 возможных сочетаний тканей.
                    </p>
                    <p><a class="btn btn-secondary" href="#">Перейти &raquo;</a></p>
                </div>
                <div class="col-md-5">
                    <img src="~/images/f2v5hs0w-stiralnaya-mashina-lg-4_600.jpg"  style="width:90%;" alt="Стиральная машина">
                </div>
            </div>

            <hr class="featurette-divider">

            <div class="row featurette">
                <div class="col-md-7 order-md-2">
                    <h2 class="featurette-heading">Холодильник SAMSUNG RB37A5291B1/WT. <span class="text-muted">Рассрочка 12 мес.</span></h2>
                    <p class="lead">
                        Храните больше продуктов с помощью технологии SpaceMax™,
                        которая позволяет добиться большего полезного внутреннего объема*,
                        благодаря тонким стенкам с теплоизоляцией, при этом не снижая энергоэффективность холодильника.
                    </p>
                    <p><a class="btn btn-secondary" href="#">Перейти &raquo;</a></p>
                </div>
                <div class="col-md-5 order-md-1">
                    <img src="~/images/rb37a5291b1wt_12.jpg" style="width:70%;"alt="Холодильник">
                </div>
            </div>

            <hr class="featurette-divider">

            <div class="row featurette">
                <div class="col-md-7">
                    <h2 class="featurette-heading">Телевизор LG 75NANO906NA. <span class="text-muted">Особая технология NanoCell.</span></h2>
                    <p class="lead">
                        Чистые цвета теперь на ультрабольшом экране.
                        С большим экраном телевизора NanoCell ваш теледосуг стал как никогда интересным.
                        Чистые цвета NanoCell создают эффектную картинку на ультрабольшом экране.
                    </p>
                    <p><a class="btn btn-secondary" href="#">Перейти &raquo;</a></p>
                </div>
                <div class="col-md-5">
                    <img src="~/images/medium03.jpg" style="width:100%;" alt="Телевизор">
                </div>
            </div>

            <hr class="featurette-divider">


            <!-- /END THE FEATURETTES -->

        </div><!-- /.container -->
    
    </main>


    <script src="/docs/5.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-b5kHyXgcpbZJO/tY9Ul7kGkf1S0CWuKcCD38l8YkeH8z8QjE0GmW1gYU5S9FOnJ0" crossorigin="anonymous"></script>


</body>
</html>